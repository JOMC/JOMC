        <groupId>org.jomc</groupId>
        <artifactId>jomc-tools</artifactId>
        <classifier>bin</classifier>
        <type>zip</type>
        <version>${jomcVersion}</version>
      </dependency>
      <dependency>
        <groupId>org.jomc</groupId>
        <artifactId>jomc-tools</artifactId>
        <classifier>bin</classifier>
        <type>tar.gz</type>
        <version>${jomcVersion}</version>
      </dependency>
      <dependency>
        <groupId>org.jomc</groupId>
        <artifactId>jomc-tools</artifactId>
